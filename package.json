{
  "name": "auto-ani-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--max-old-space-size=1024' next dev",
    "build": "NODE_OPTIONS='--expose-gc --max-old-space-size=2048' next build",
    "start": "NODE_OPTIONS='--expose-gc --max-old-space-size=1024' next start -H 0.0.0.0 -p ${PORT:-3000}",
    "lint": "next lint",
    "build:production": "NODE_OPTIONS='--expose-gc --max-old-space-size=2048' npx prisma generate && npm run build",
    "build:staging": "npx prisma generate && npm run build",
    "build:preview": "npx prisma generate && npm run build",
    "db:generate": "prisma generate",
    "db:generate:dev": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:seed:production": "NODE_ENV=production tsx prisma/seed.ts",
    "db:seed:quick": "node scripts/quick-seed.js",
    "db:seed:direct": "node scripts/seed-vehicles-directly.js",
    "db:studio": "prisma studio",
    "db:test": "node scripts/test-db-connection.js",
    "db:health": "node scripts/db-health-check.js",
    "db:backup": "node scripts/db-backup.js",
    "db:backup:list": "node scripts/db-backup.js list",
    "db:backup:clean": "node scripts/db-backup.js clean",
    "db:migrate:prod": "node scripts/db-migrate-prod.js",
    "db:push-prod": "bash scripts/push-to-supabase.sh",
    "memory:validate": "tsx scripts/validate-memory-fixes.ts",
    "memory:monitor": "tsx --expose-gc scripts/validate-memory-fixes.ts",
    "deploy:new": "bash scripts/deploy-new-netlify.sh",
    "postinstall": "prisma generate",
    "type-check": "tsc --noEmit",
    "lint:strict": "next lint --max-warnings 0",
    "build:full": "npm run type-check && npm run lint:strict && npm run build",
    "test:build": "npm run db:generate && npm run type-check && npm run build"
  },
